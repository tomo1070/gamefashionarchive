<div class="row">
  <% @posts.each do |post| %>
    <div class="col-md-2">
      <%=link_to "グループを作成する",new_group_path %> | 
      <%=link_to "グループ一覧",groups_path %>
    </div>
    <div class="col-md-4">
      <%= link_to list_path(post.id) do %>
        <%= image_tag post.get_image %>
      <% end %>
    </div>
  <% end %>
</div>
<%= paginate @posts %>